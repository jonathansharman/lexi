<h1>
{{- lexicon.format.native.prefix -}}
{{- lexicon.format.native.title -}}
{{- lexicon.format.native.suffix -}}
</h1>
{% for lexeme in lexicon.lexemes -%}
{%- if lexeme.definition %}
<div id="{{ lexeme.lemma }}">
	<b><a href="#{{ lexeme.lemma }}">
	{{- lexicon.format.native.prefix -}}
	{{- lexeme.lemma -}}
	{{- lexicon.format.native.suffix -}}
	</a></b> Â· {{ lexicon.format.native.prefix -}}
	{{- lexeme.class -}}
	{{- lexicon.format.native.suffix }}
	<div style="margin-left:{{ lexicon.format.indent }}">
		{{- lexicon.format.native.prefix -}}
		{{- lexeme.definition -}}
		{{- lexicon.format.native.suffix -}}
	</div>
</div>
{% endif -%}
{%- endfor -%}
